{{#if issues}}
  <ul id="sentry-issues">

  {{#each issues}}

    <li>
      <strong><a href="{{this.permalink}}" target="_blank">{{this.title}}</a></strong>
      <div class="last-seen">{{this.lastSeen}} ago - {{this.firstSeen}} old</div>
    </li>

  {{/each}}

  </ul>

{{else}}
  <p>None found</p>
{{/if}}
