{
  "author": {
    "name": "Sean Lynch",
    "email": "sean@lyn.ch",
    "url": "https://github.com/sean-lynch"
  },
  "domainWhitelist": ["sentry.io"],
  "parameters": [
    {
      "label": "Sentry Auth Token",
      "name": "token",
      "type": "text",
      "secure": true,
      "required": true,
      "helpText": "Provide your Sentry authentication token"
    },
    {
      "label": "Sentry organization slug",
      "name": "orgName",
      "type": "text",
      "required": true,
      "helpText": "\"slug\" name of your organization"
    },
    {
      "label": "Sentry project slug",
      "name": "projectName",
      "type": "text",
      "required": true,
      "helpText": "\"slug\" name of your project"
    },
    {
      "label": "API host",
      "name": "host",
      "type": "hidden",
      "default": "https://sentry.io"
    }
  ],
  "defaultLocale": "en",
  "private": false,
  "location": ["ticket_sidebar", "user_sidebar"],
  "version": "1.0",
  "frameworkVersion": "1.0"
}
